# Production MinIO Environment Configuration
# This file shows production patterns - DO NOT USE THESE VALUES DIRECTLY

# Production MinIO Cluster
MINIO_ENDPOINT=https://minio-prod.company.com
MINIO_ACCESS_KEY=${VAULT_MINIO_ACCESS_KEY}
MINIO_SECRET_KEY=${VAULT_MINIO_SECRET_KEY}
MINIO_REGION=us-west-2

# Production Iceberg Catalog (REST-based)
PYICEBERG_CATALOG__PROD__TYPE=rest
PYICEBERG_CATALOG__PROD__URI=https://iceberg-catalog.company.com
PYICEBERG_CATALOG__PROD__WAREHOUSE=s3://prod-iceberg-warehouse/
PYICEBERG_CATALOG__PROD__S3__ENDPOINT=https://minio-prod.company.com
PYICEBERG_CATALOG__PROD__S3__ACCESS_KEY_ID=${VAULT_MINIO_ACCESS_KEY}
PYICEBERG_CATALOG__PROD__S3__SECRET_ACCESS_KEY=${VAULT_MINIO_SECRET_KEY}
PYICEBERG_CATALOG__PROD__S3__REGION=us-west-2
PYICEBERG_CATALOG__PROD__S3__PATH_STYLE_ACCESS=true

# Security settings
PYICEBERG_CATALOG__PROD__S3__SSL_VERIFY=true
PYICEBERG_CATALOG__PROD__S3__CONNECT_TIMEOUT=30
PYICEBERG_CATALOG__PROD__S3__READ_TIMEOUT=60

# Monitoring and logging
LOG_LEVEL=INFO
METRICS_ENABLED=true
TRACING_ENABLED=true